<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_memberedit"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.paul.m_branch.memberedit">
<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="No"
    android:id="@+id/mno"
    android:layout_centerHorizontal="true"
    />
    <TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="Name"
    android:id="@+id/mname"
        android:layout_below="@id/mno"
    android:layout_centerHorizontal="true"
    />
<EditText
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_centerHorizontal="true"
    android:layout_below="@+id/mname"
    android:hint="Id no"
    android:id="@+id/idno"/>
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/idno"
        android:hint="Phone number"
        android:id="@+id/phone"/>
<Button
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/updatemember"
    android:layout_centerHorizontal="true"
    android:layout_below="@id/phone"
    android:text="Update"/>
    <Button
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/cancel"
    android:layout_centerHorizontal="true"
    android:layout_below="@id/updatemember"
    android:text="Cancel"/>

</RelativeLayout>
