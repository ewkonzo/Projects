<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wizarpos.log.activity"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="18" />

     <!-- 在指定activity上屏蔽状态栏按钮 -->
    <uses-permission android:name="android.permission.CLOUDPOS_DISABLE_HOME_KEY_IN_ACTIVITY" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/display_128"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.wizarpos.test.activity.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.wizarpos.test.activity.BtnpressActivity"
             android:theme="@android:style/Theme.DeviceDefault.Dialog">
        </activity>
    </application>

</manifest>