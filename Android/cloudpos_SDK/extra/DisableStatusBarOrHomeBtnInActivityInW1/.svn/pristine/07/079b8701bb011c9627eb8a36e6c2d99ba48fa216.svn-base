<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="wrap_content"    android:layout_height="wrap_content"    android:orientation="horizontal" >    <LinearLayout        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:orientation="vertical" >                 <com.wizarpos.barcode.scanner.ScannerRelativeLayout                android:id="@+id/qrc_scanner"                android:layout_width="550dp"                android:layout_height="330dp"                 />               <LinearLayout                android:layout_width="match_parent"                android:layout_height="match_parent"                android:orientation="horizontal" >                <Button                    android:id="@+id/qrc_renew"                    android:layout_width="0dp"                    android:layout_height="70dp"                    android:layout_weight="1.0"                    android:background="@drawable/button_selector"                    android:text="@string/re_new"                    android:textSize="40sp" />                <Button                    android:id="@+id/qrc_end"                    android:layout_width="0dp"                    android:layout_height="70dp"                    android:layout_weight="1.0"                    android:background="@drawable/button_selector"                    android:text="@string/end"                    android:textSize="40sp" />            </LinearLayout>        </LinearLayout>        <LinearLayout            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:orientation="horizontal" >            <TextView                android:id="@+id/qrc_text_result"                android:layout_width="240dp"                android:layout_height="410dp"                android:textSize="40sp" />        </LinearLayout>    </LinearLayout>